{
    "env": {
      "browser": true
    },
    "extends": [
      "standard",
      "prettier"
    ],
    "parserOptions": {
      "ecmaVersion": "latest",
      "sourceType": "module"
    },
    "rules": {
        "react/jsx-uses-react": "off",
        "react/react-in-jsx-scope": "off"
    },
    "settings": {
        "react": {
          "version": "detect" // Reactのバージョンを自動的に検出する
        }
      }
  }